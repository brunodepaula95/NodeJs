<!DOCTYPE html>
<html lang="en">
<%- include('.partials/head.ejs') %>
<body>
  <!--<% const message = 'Hello world' %>

  <h1><%= message %></h1>-->

  <div class="container">
    <div class="content">
      <% if (users.length > 0) { %> <% users.forEach(user => { %>

        <h3 class="tittle">
          <%= user.firstName %> <%= user.lastName %>
        </h3>

        <p> <%= user.email %></p>

        <% }) %> <% } else { %>
          <p>Não há usuarios</p>
          <% } %>
    </div>
  </div>
</body>
</html>